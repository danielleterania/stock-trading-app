<%= form_with(model: @user, local: true) do |form| %>
    <!-- Other fields -->
  
    <div class="field">
      <%= form.label :user_type %>
      <%= form.select :user_type, options_for_select([['Admin', 'admin'], ['Trader', 'trader']], @user.user_type) %>
    </div>
  
    <div class="actions">
      <%= form.submit %>
    </div>
  <% end %>
  